        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wanliang.site.repository.UserRepository">
    <select id="findUserByUserName" resultType="com.wanliang.site.domain.sys.User">
        select * from member where username = #{username}
